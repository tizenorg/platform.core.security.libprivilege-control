#!/bin/sh

DBSPACE=/opt/dbspace
DBSPACE_RW=/opt/usr/dbspace
APPS_DIR=/opt/apps
SHARE_DIR=/opt/share

chown 5000:5000 /home/app



if [ ! -e /opt/etc/.pkg_smack ]; then
	/usr/bin/pkg_smack
	touch /opt/etc/.pkg_smack
fi

# set vconf label when first boot
#if [ ! -e /opt/etc/.vconf_labeling ]; then
#	touch /opt/etc/.vconf_labeling
#fi


